
@{
    ViewBag.Title = "_InstructionList";
}

@model IEnumerable<WebApplication1.Models.ERP.Instruction>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>編號</th>
            <th>標題</th>
            <th>內容</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Id</td>
                <td>@i.Title</td>
                <td>@i.Description</td>
            </tr>
        }
    </tbody>
</table>